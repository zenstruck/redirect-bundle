{
    "name": "zenstruck/redirect-bundle",
    "description": "Store redirects for your site and keeps statistics on redirects and 404 errors",
    "keywords": ["redirect"],
    "homepage": "http://zenstruck.com/project/ZenstruckRedirectBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Bond",
            "email": "kevinbond@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "symfony/framework-bundle": "^2.8|^3.0|^4.0",
        "symfony/doctrine-bridge": "^2.8|^3.0|^4.0",
        "symfony/dependency-injection": "^2.8|^3.0|^4.0",
        "doctrine/doctrine-bundle": "^1.3|^2.0",
        "doctrine/orm": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.11",
        "matthiasnoback/symfony-dependency-injection-test": "^1.2",
        "symfony/finder": "^2.8|^3.0|^4.0",
        "symfony/browser-kit": "^2.8|^3.0|^4.0",
        "symfony/css-selector": "^2.8|^3.0|^4.0",
        "symfony/form": "^2.8|^3.0|^4.0",
        "symfony/validator": "^2.8|^3.0|^4.0",
        "symfony/translation": "^2.8|^3.0|^4.0",
        "symfony/console": "^2.8|^3.0|^4.0"
    },
    "autoload": {
        "psr-4": { "Zenstruck\\RedirectBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Zenstruck\\RedirectBundle\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
